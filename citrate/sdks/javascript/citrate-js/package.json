{
  "name": "citrate-js",
  "version": "0.1.3",
  "description": "JavaScript/TypeScript SDK for Citrate AI blockchain platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --testPathIgnorePatterns=tests/integration",
    "test:unit": "jest --testPathIgnorePatterns=tests/integration",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts' --runInBand --testTimeout=120000",
    "test:integration:devnet": "CITRATE_RPC_URL=http://localhost:8545 CITRATE_CHAIN_ID=1337 npm run test:integration",
    "test:all": "jest --testTimeout=120000",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "citrate",
    "blockchain",
    "ai",
    "ml",
    "ethereum",
    "web3",
    "typescript",
    "javascript"
  ],
  "author": "Citrate Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/citrate-ai/citrate.git",
    "directory": "sdks/javascript/citrate-js"
  },
  "bugs": {
    "url": "https://github.com/citrate-ai/citrate/issues"
  },
  "homepage": "https://docs.citrate.ai",
  "dependencies": {
    "axios": "^1.5.0",
    "crypto-js": "^4.1.1",
    "elliptic": "^6.5.4",
    "ethers": "^6.8.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/elliptic": "^6.4.16",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
