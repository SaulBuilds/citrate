{
  "name": "lattice-docs",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_LATTICE_RPC": "https://rpc.lattice.ai",
    "NEXT_PUBLIC_IPFS_URL": "https://ipfs.lattice.ai"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_LATTICE_RPC": "https://rpc.lattice.ai",
      "NEXT_PUBLIC_IPFS_URL": "https://ipfs.lattice.ai"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}