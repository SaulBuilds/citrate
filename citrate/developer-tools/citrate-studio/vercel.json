{
  "name": "citrate-studio",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_LATTICE_RPC": "https://rpc.citrate.ai",
    "REACT_APP_IPFS_URL": "https://ipfs.citrate.ai",
    "REACT_APP_WEBSOCKET_URL": "wss://ws.citrate.ai"
  },
  "build": {
    "env": {
      "REACT_APP_LATTICE_RPC": "https://rpc.citrate.ai",
      "REACT_APP_IPFS_URL": "https://ipfs.citrate.ai",
      "REACT_APP_WEBSOCKET_URL": "wss://ws.citrate.ai"
    }
  },
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}