{
  "name": "citrate-debug-dashboard",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_LATTICE_RPC": "https://rpc.lattice.ai",
    "VITE_WEBSOCKET_URL": "wss://ws.lattice.ai"
  },
  "build": {
    "env": {
      "VITE_LATTICE_RPC": "https://rpc.lattice.ai",
      "VITE_WEBSOCKET_URL": "wss://ws.lattice.ai"
    }
  }
}